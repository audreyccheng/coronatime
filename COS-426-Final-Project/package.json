{
  "name": "coronatime",
  "repository": "coronatime",
  "version": "0.1.0",
  "title": "CoronaTime",
  "description": "Game in which you (a white blood cell) travel through a blood vessel and try to earn as many points as possible by traveling as far as possible (avoiding collisions with blood clots) and eating (colliding with) as many viruses as possible.",
  "keywords": [
    "Princeton",
    "graphics",
    "three.js",
    "COS 426",
    "infinite cylinder",
    "corona virus"
  ],
  "scripts": {
    "start": "webpack-dev-server --mode development --output-public-path '/'",
    "prebuild": "rimraf ./build && mkdir ./build",
    "build": "npm run prebuild && webpack --mode production --output-public-path '/'",
    "format": "prettier --write '*.js' 'src/**/*.js'",
    "deploy": "npm run prebuild && webpack --mode production && gh-pages -d build"
  },
  "dependencies": {
    "dat.gui": "^0.7.7",
    "electron-spawn": "^5.0.0",
    "node-mp3-player": "^1.0.3",
    "play-audio-url": "^1.0.1",
    "play-sound": "^1.1.3",
    "postprocessing": "^6.13.5",
    "three": "^0.125.0",
    "three-obj-loader": "^1.1.3",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "gh-pages": "^2.2.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.1",
    "prettier": "^2.0.4",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.10.3"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
